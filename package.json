{
  "name": "package.json",
  "version": "2.0.0",
  "description": "A Discord bot for Con of the North's Discord server",
  "author": "OmneGeek, Yiggles",
  "dependencies": {
    "@discordjs/opus": "github:discordjs/opus",
    "discord.js": "^12.5.1",
    "discord.js-commando": "git+https://github.com/discordjs/commando.git",
    "dotenv": "^8.2.0",
    "module-alias": "^2.2.2",
    "node-fetch": "^2.6.1"
  },
  "_moduleAliases": {
    "@root": ".",
    "@audio": "./audio"
  },
  "main": "./dist/bot.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node bot.ts",
    "prod": "tsc && node ./dist/bot.js",
    "start": "nodemon ./dist/bot.js",
    "format": "prettier-standard '**/*.ts'",
    "lint": "eslint . --ext .ts"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^14.14.28",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5"
  }
}
